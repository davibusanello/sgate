<h1>Listing agendas</h1>

<table>
  <tr>
    <th>Data agendada</th>
    <th>Data realizada</th>
    <th>Vendedor</th>
    <th>Instalador</th>
    <th>Pacote</th>
    <th>Descricao</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @agendas.each do |agenda| %>
  <tr>
    <td><%= agenda.data_agendada %></td>
    <td><%= agenda.data_realizada %></td>
    <td><%= agenda.vendedor_id %></td>
    <td><%= agenda.instalador_id %></td>
    <td><%= agenda.pacote_id %></td>
    <td><%= agenda.descricao %></td>
    <td><%= link_to 'Show', agenda %></td>
    <td><%= link_to 'Edit', edit_agenda_path(agenda) %></td>
    <td><%= link_to 'Destroy', agenda, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Agenda', new_agenda_path %>
